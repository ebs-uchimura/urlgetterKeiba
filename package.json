{
  "name": "test.com.urlgetterkeiba",
  "version": "1.0.0",
  "description": "getting horse racing data.",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && electron ./dist/main.js",
    "build:mac": "electron-builder --mac --x64",
    "build:win": "electron-builder --win --x64"
  },
  "author": "Koichi Uchimura",
  "license": "ISC",
  "dependencies": {
    "csv-parse": "^4.16.0",
    "csv-stringify": "^5.6.2",
    "fs": "^0.0.1-security",
    "iconv-lite": "^0.6.3",
    "path": "^0.12.7",
    "puppeteer-core": "^10.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^17.0.31",
    "electron": "^13.6.9",
    "electron-builder": "^22.11.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "build": {
    "appId": "jp.scrape.app1",
    "files": [
      "!**/node_modules/*"
    ],
    "win": {
      "target": {
        "target": "dir",
        "arch": [
          "x64"
        ]
      }
    },
    "mac": {
      "target": "dmg"
    }
  }
}
